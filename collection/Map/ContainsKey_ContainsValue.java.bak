import java.util.*;

class ContainsKey_ContainsValue
{
	public static void main(String[] args) 
	{
		HashMap hm=new HashMap();
		hm.put(0,"Hello");
		hm.put(1,"Hi");
		hm.put(null,null);
		hm.put(2,"Hey");
		hm.put(2,"How r u");
		hm.put(3,null);
		System.out.println(hm);
		System.out.println(hm.containsKey(1));
		System.out.println(hm.containsValue("hey"));
		System.out.println(hm.containsValue("Hi"));
	}
}
